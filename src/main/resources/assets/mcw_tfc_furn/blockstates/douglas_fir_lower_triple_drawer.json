    },
	{
      "when": { "AND": [  {"facing": "west","north": "false","east": "true","west": "true","south": "false"} ]},
      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_triple_shelf_middle","uvlock": true, "y": 90, "x": 180}
    },
	{
      "when": { "AND": [  {"facing": "west","north": "false","east": "false","west": "true","south": "false"} ]},
      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_triple_shelf_right","uvlock": true, "y": 90, "x": 180}
