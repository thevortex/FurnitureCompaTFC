      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_triple_shelf_single","uvlock": true, "x": 180}
    },
	{
      "when": { "AND": [  {"facing": "south","north": "true","east": "false","west": "false","south": "true"} ]},
      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_triple_shelf_middle","uvlock": true, "x": 180}
    },
	{
      "when": { "AND": [  {"facing": "south","north": "false","east": "false","west": "false","south": "true"} ]},
