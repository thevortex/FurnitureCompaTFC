{
  "multipart": [
  
  
    {
      "when": { "AND": [  {"facing": "north|south"} ]},
      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_drawer_base","uvlock": true}
    },
    {
      "when": { "AND": [  {"facing": "east|west"} ]},
      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_drawer_base","uvlock": true, "y": 90}
    },
	
	
	{
      "when": { "AND": [  {"facing": "north","north": "false","east": "false","west": "false","south": "false"} ]},
      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_triple_shelf_single","uvlock": true, "y": 180, "x": 180}
    },
	{
      "when": { "AND": [  {"facing": "north","north": "true","east": "false","west": "false","south": "true"} ]},
      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_triple_shelf_middle","uvlock": true, "y": 180, "x": 180}
    },
	{
      "when": { "AND": [  {"facing": "north","north": "true","east": "false","west": "false","south": "false"} ]},
      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_triple_shelf_right","uvlock": true, "y": 180, "x": 180}
    },
	{
      "when": { "AND": [  {"facing": "north","north": "false","east": "false","west": "false","south": "true"} ]},
      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_triple_shelf_left","uvlock": true, "y": 180, "x": 180}
    },
	
	{
