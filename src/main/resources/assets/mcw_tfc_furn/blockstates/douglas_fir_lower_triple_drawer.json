    },
	{
      "when": { "AND": [  {"facing": "east","north": "false","east": "true","west": "true","south": "false"} ]},
      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_triple_shelf_middle","uvlock": true, "y": 270, "x": 180}
    },
	{
      "when": { "AND": [  {"facing": "east","north": "false","east": "true","west": "false","south": "false"} ]},
      "apply": { "model": "mcw_tfc_furn:block/drawer/douglas_fir_triple_shelf_right","uvlock": true, "y": 270, "x": 180}
